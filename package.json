{
  "name": "xbounty",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "clean": "pnpm -r clean",
    "registry": "pnpm --filter @xbounty/registry start",
    "registry:dev": "pnpm --filter @xbounty/registry dev",
    "registry:test": "pnpm --filter @xbounty/registry test",
    "creation": "pnpm --filter @xbounty/creation generate",
    "creation:once": "pnpm --filter @xbounty/creation generate:once",
    "creation:test": "pnpm --filter @xbounty/creation test",
    "profile": "pnpm --filter @xbounty/profile dev",
    "profile:start": "pnpm --filter @xbounty/profile start",
    "suggestion": "pnpm --filter @xbounty/suggestion dev",
